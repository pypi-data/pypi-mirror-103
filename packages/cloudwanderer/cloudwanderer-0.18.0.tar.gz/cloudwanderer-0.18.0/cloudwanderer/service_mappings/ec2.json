{
    "service": {
        "globalService": false,
        "regionalResources": true
    },
    "resources": {
        "Vpc": {
            "type": "resource"
        },
        "VpcEnableDnsSupport": {
            "type": "secondaryAttribute"
        },
        "Image": {
            "type": "resource",
            "defaultFilters": {
                "Owners": [
                    "self"
                ]
            }
        },
        "Snapshot": {
            "type": "resource",
            "defaultFilters": {
                "OwnerIds": [
                    "self"
                ]
            }
        }
    }
}

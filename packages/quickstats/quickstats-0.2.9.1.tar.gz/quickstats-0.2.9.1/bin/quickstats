#! /usr/bin/env python

import click
from quickstats.components.cli import run_pulls, plot_pulls, likelihood_scan, compile_macros

@click.group()
def cli():
    pass

if __name__ == "__main__":
    cli.add_command(compile_macros)   
    cli.add_command(run_pulls)
    cli.add_command(plot_pulls)
    cli.add_command(likelihood_scan)   
    cli()